    <title>Sign-In</title>
</head>
<body>

    <header>
        <h1>OWN<span id="ex">Sheet</span></h1>
    </header>
    <main>
        <div id = "left">
            <div id="lefthead">
                <h1>Make Your Own SDE sheet</h1>
            </div>
            <div id="Img">
                <!-- <img src="img/travel.webp" alt=""> -->
            </div>
        </div>
        <section id="right">
            <div id="login">
                <form action="/create/sign-in" method="post">
                    <h1>Sign-In</h1>
                    <input type="email" name="email" placeholder="username/email" required>
                    <input type="password" name="password" placeholder="password" required>
                    <input type="submit" value="Login" id="login-btn" style="cursor: pointer;">
                </form>
                <div id="line">
                    <div id="line1"></div>
                    <div id="or">OR</div>
                    <div id="line2"></div>
                </div>
                <div>
                    <h2>
                        <a href="#">Forgot Password?</a>
                    </h2>
                </div>
            </div>
            <div id="sign-up">
                <h3>
                    Don't have an account? 
                    <a href="/sign-up">Sign Up</a>
                </h3>
            </div>
        </section>
    </main>
    

  
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
